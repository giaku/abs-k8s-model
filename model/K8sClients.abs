module K8sClients;

export *;

import * from K8sWorkflow;

interface Client{
  Unit start();
}
class ClientObject(ServiceTask t,Int times,Rat delay) implements Client {

  Unit start(){
    Int counter = times;

    while(counter > 0){
      t!execute();
      counter = counter - 1;
      await duration(delay,delay);
    }
  }

}
